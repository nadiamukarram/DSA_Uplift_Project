/***
number pattern
i/p: number
7
o/p:

7 6 5 4 3 2 1 
7 6 5 4 3 2 
7 6 5 4 3 
7 6 5 4 
7 6 5 
7 6 
7

**/

#include <iostream>
using namespace std;

int
main ()
{
  int n, i, j;
  cin >> n;
  for (i = 0; i < n; i++)
    {
      for (j = n; j > i; j--)
	{
	  cout << j << " ";
	}
      cout << endl;
    }
  return 0;
}
