*******************************************************************************/
#include <iostream>
using namespace std;

int
main ()
{
  int n, i, j;
  cin >> n;
  for (i = 0; i < n; i++)
    {
      for (j = n; j > i; j--)
	{
	  cout << j << " ";
	}
      cout << endl;
    }
  return 0;
}

