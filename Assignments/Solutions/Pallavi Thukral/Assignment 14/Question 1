https://practice.geeksforgeeks.org/problems/count-nodes-of-linked-list/1/?track=DSASP-LinkedList&batchId=154

class Solution
{
    public:
    //Function to count nodes of a linked list.
    int getCount(struct Node* head){
    
        //Code here
        Node *temp = head;
        int length=0;
        while(temp!=NULL)
        {   
            length++;
            temp=temp->next;
        }
        return length;
    
    }
};
