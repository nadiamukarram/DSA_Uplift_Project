https://practice.geeksforgeeks.org/problems/at-least-two-greater-elements4625/1


class Solution{
public:
        vector<int> findElements(int a[], int n)
    {
        vector<int>v;
        // Your code goes here
        sort(a,a+n);
        for(int i=0;i<n-2;i++)
        v.push_back(a[i]);
        
        return v;
    }
};
