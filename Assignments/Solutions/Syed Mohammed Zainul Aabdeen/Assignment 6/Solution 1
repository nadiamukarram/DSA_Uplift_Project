#include <iostream>
using namespace std;
int main() {
    int n,x,flag=0;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    cin>>a[i];
    cout<<"enter no.to find: ";
    cin>>x;
    for(int i=0;i<n;i++){
        if(a[i]==x){
            flag=1;
            cout<<"the no.is at index: "<<i;
            break;
        }
    }
    if(flag==0)
    cout<<"the no. is not there";
    return 0;
}
