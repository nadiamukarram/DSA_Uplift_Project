#include<stdio.h>
int main()
{
    int temp, n;
    scanf("%d", &n);
    int a[n];
    for(int i=0; i<n; i++)
        scanf("%d", &a[i]);

    for(int j=0; j<n-1; j++)
    {
        for(int i=0; i<n; i++)
        {
            if(a[i] > a[i+1])
            {
                temp = a[i];
                a[i] = a[i+1];
                a[i+1] = temp;
            }
        }
    }
    for(int i=0; i<n; i++)
        printf("%d", a[i]);
}

