#include<stdio.h>
void rotate_array(int a[], int n)
{
    int i=0, j=n-1, temp;
    while(i!=j)
    {
        temp = a[i];
        a[i] = a[j];
        a[j] = temp;
        i++;
    }
}

int main()
{
    int a[] = {1,2,3,4,5}, i;
    int n = sizeof(a)/sizeof(a[0]);
    for (i = 0; i < n; i++)
        printf("%d", a[i]);
    printf("\n");
    
    rotate_array(a, n);
    for (i = 0; i < n; i++)
        printf("%d", a[i]);
    return 0;
}

