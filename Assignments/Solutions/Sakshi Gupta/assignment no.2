#include <iostream>
using namespace std;
int main()
{
    int a[] = {-2, -3, 4, -1, -2, 1, 5, -3};
    int  n = sizeof(a)/sizeof(a[0]),lmax=a[0], gmax=a[0];
    for(int i=1; i<n; i++)
    {
        if(a[i]>(a[i]+lmax))
        {
            lmax = a[i];
        }
        else
            lmax = lmax + a[i];
        gmax = max(lmax, gmax);
    }
    cout<<gmax<<endl;
    return 0;
}

